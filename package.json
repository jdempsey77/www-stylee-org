{
  "name": "www-stylee-org",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:staging": "NODE_ENV=staging CUSTOM_DOMAIN=false next build",
    "build:prod": "NODE_ENV=production next build",
    "export": "next build",
    "export:staging": "NODE_ENV=staging CUSTOM_DOMAIN=false next build",
    "export:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:staging": "NODE_ENV=staging next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "test:build": "npm run build:staging && npm run validate:build",
    "validate:build": "node scripts/validate-build.js",
    "serve:staging": "npx serve out -p 3001",
    "preview": "npm run build:staging && npm run serve:staging",
    "test:local": "npm run lint && npm run audit && npm run test:build",
    "test:quick": "npm run lint && npm run build:staging",
    "dev:test": "npm run lint && npm run dev",
    "staging:full": "npm run test:local && npm run preview",
    "dev:workflow": "node scripts/dev-workflow.js",
    "serve:local": "./scripts/serve-local.sh",
    "watch:files": "./scripts/watch-files.sh",
    "test:blog": "./scripts/test-blog.sh",
    "preview:blog": "./scripts/dev-helper.sh preview",
    "dev:full": "./scripts/dev-helper.sh full",
    "resume:update": "node scripts/resume-pipeline.js",
    "resume:build": "npm run resume:update && npm run build:staging",
    "resume:preview": "npm run resume:build && npm run serve:staging",
    "resume:simple": "node scripts/simple-resume-pipeline.js",
        "resume:google": "node scripts/google-drive-pipeline.js",
        "resume:google-api": "node scripts/google-drive-api-pipeline.js",
        "test:google-drive": "node scripts/test-google-drive-setup.js",
        "resume:hybrid": "node scripts/hybrid-resume-pipeline.js",
        "pipeline:production": "node scripts/production-pipeline.js",
        "setup:hooks": "./scripts/setup-pre-push-hook.sh",
        "test:production": "npm run pipeline:production"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "googleapis": "^160.0.0",
    "gtag": "^1.0.1",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "puppeteer": "^24.22.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
